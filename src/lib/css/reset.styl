@require '~nib/lib/nib/reset.styl'
global-reset()
* {
  box-sizing: border-box;
}
a{
  text-decoration: none
}


body{
  word-break: break-word;
}
ol{
  list-style: decimal
}
select, button, input, textarea {
  font-size: inherit;
}
button{
  padding: 0;
  border-width: 0;
  outline: none;
  background-color: transparent;
  color: inherit;
}

