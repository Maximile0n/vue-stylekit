.color-block
  &--white
    background-color #fff
  &--dark-state
    background-color b2b-dark-state
  &--purple
    background-color b2c-purple
  &--purple-low
    background-color b2c-purple-low
  &--purple-hight
    background-color b2c-purple-hight
  &--red
    background-color b2c-red
  &--red-low
    background-color b2c-red-low
  &--red-hight
    background-color b2c-red-hight
  &--dark-blue
    background-color b2c-dark-blue
  &--dark-blue-low
    background-color b2c-dark-blue-low
  &--dark-blue-hight
    background-color b2c-dark-blue-hight
  &--light-blue
    background-color b2c-light-blue
  &--light-blue-low
    background-color b2c-light-blue-low
  &--light-blue-hight
    background-color b2c-light-blue-hight
  &--yellow
    background-color b2c-yellow
  &--yellow-low
    background-color b2c-yellow-low
  &--yellow-hight
    background-color b2c-yellow-hight
  &--orange
    background-color b2c-orange
  &--orange-low
    background-color b2c-orange-low
  &--orange-hight
    background-color b2c-orange-hight
  &--pink
    background-color b2c-pink
  &--pink-low
    background-color b2c-pink-low
  &--pink-hight
    background-color b2c-pink-hight
  &--green
    background-color b2c-green
  &--green-low
    background-color b2c-green-low
  &--green-hight
    background-color b2c-green-hight
  &--black
    background-color b2c-black

.color-block--light-grey
  background-color: light-grey

.color-block--rule-color
  background-color rule-color

.color-block--rule-color
  background-color: #272F3D

