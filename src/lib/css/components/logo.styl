.rt-logo
  display: inline-block
  svg
    max-width: 100%
.rt-logo-line
  display: inline-block
  width: 1px
  padding-left: horizontal-space
  padding-right: horizontal-space
  height: 115px
  position: relative
  @media (max-width: tablet-upper-limit)
    height: 70px
    top: -10px
  @media (max-width: mobile-upper-limit)
    height: 55px
    top: -20px
  &:before
    content: ''
    position: absolute
    width: 1px
    height: 100%
    background-color: #fff
    left: 0
    right: 0
    top: 0
    bottom: 0
    margin-left: auto
    margin-right: auto
.rt-logo-co-branding
  position: relative
  display: table
  flex-direction row
  margin-left: -10px
  margin-right: -10px

.rt-logo-co-branding__item
  display: table-cell
  padding-left: 8px
  padding-right: 8px
.rt-logo-co-branding__line
  display: table-cell
  height: 100%
  width: 1px

  padding-top: 6px
  position: relative
  bottom: -3px
  margin-left: 10px
  &:before
    content: ''
    position: absolute
    top: -6px
    bottom: -3px
    width: 100%

    background-color: #878C93
    display: block


.rt-logo__rt-text
  margin-left: -5%
  margin-bottom: 5.5%
