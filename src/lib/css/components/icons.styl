.rt-icon
  opacity 1
  visibility visible
  transition-timing-function ease-in-out
  transition-duration .4s
  transition-property opacity, visibility

  &__wrapper
    display: inline-block

  &.wc-icon
    border: none
    border-radius: 50%
    fill: transparent
    position: relative
    stroke: #FFF
    text-align: center
    vertical-align: middle
    *
      fill: inherit
      stroke: inherit
  &.wc-candy-icon
    padding: 0
    [pid="0"]
      stroke: transparent
    g
      opacity: 1

  .caption.top
    background: inherit
    border: 2px solid #fff
    border-radius: 10px
    color: white
    font-size: 12px;
    line-height: 13px
    height: 21px
    padding: 2px 4px
    position: absolute
    top: -8px
    right: -2px

  .icon-speed__spinned-arrow
    margin: auto
    position: absolute
    width: 100%
    height: 100%
    top: 0
    left: 0
    right: 0
    bottom: 0
    svg
      margin: auto
      position: absolute
      width: 100%
      height: 100%
      top: 0
      left: 0
      right: 0
      bottom: 0
    circle
      stroke-width: 1.4px
      stroke: #ffffff
      transition: stroke-dashoffset 2s ease
    path
      transition: transform 2s ease

